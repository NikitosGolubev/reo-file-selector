{
  "name": "reo-file-selector",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "prepare": "husky install",
    "postinstall": "husky install",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|tsx|vue)\"",
    "format": "prettier --write --ignore-path .gitignore \"src/**/*.+(ts|tsx|vue)\"",
    "check-format": "prettier --list-different --ignore-path .gitignore \"src/**/*.+(ts|tsx|vue)\"",
    "types-check": "tsc --noEmit",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "ant-design-vue": "^2.2.8",
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5"
  }
}
